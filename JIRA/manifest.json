{
  "manifest_version": 3,
  "name": "JIRA Copy Tickett",
  "version": "1.0.1",
  "description": "Adds a button to copy JIRA ticket key to clipboard.",
  "key": "",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
    "clipboardWrite"
  ],
  "content_scripts": [
    {
      "matches": [
          "https://*/browse/*"
      ],
      "js": ["content.js"],
      "css": ["content.css"]
    }
  ]
}
